{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "baseUrl": ".",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/types/shared": [
        "./types/shared/index.ts"
      ],
      "@/types/shared/*": [
        "./types/shared/*"
      ],
      "@/types/client": [
        "./types/client/index.ts"
      ],
      "@/types/client/*": [
        "./types/client/*"
      ],
      "@/types/server": [
        "./types/server/index.ts"
      ],
      "@/types/server/*": [
        "./types/server/*"
      ],
      "@/types/backend": [
        "./types/backend/index.ts"
      ],
      "@/types/backend/*": [
        "./types/backend/*"
      ],
      "react-icons/fa": [
        "./stubs/react-icons-fa.tsx"
      ],
      "react-icons/si": [
        "./stubs/react-icons-si.tsx"
      ],
      "react-icons/vsc": [
        "./stubs/react-icons-vsc.tsx"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "data/projects",
    "external_Claudable"
  ]
}
